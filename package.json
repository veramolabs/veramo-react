    "@veramo/remote-client": "3.1.2-next.91",
    "@veramo/selective-disclosure": "3.1.2-next.91",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.2",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react-hooks": "5.0.3",
    "@types/jest": "26.0.20",
    "@types/react": "17.0.0",
    "@types/react-test-renderer": "17.0.0",
    "@types/uuid": "8.3.0",
    "babel-jest": "26.6.3",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-test-renderer": "17.0.1",
    "semantic-release": "17.4.3",
    "ts-jest": "26.4.4",
    "typescript": "4.0.3"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -b --watch",
    "test": "jest --config=jest.json --coverage=false",
    "test:watch": "yarn test --watch --verbose",
    "upgrade-veramo": "yarn add @veramo/core@next @veramo/remote-client@next @veramo/credential-w3c@next @veramo/data-store@next @veramo/selective-disclosure@next @veramo/did-comm@next",
    "release": "semantic-release"
  },
  "prettier": {
    "jsxBracketSameLine": false,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": true,
    "semi": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Simonas Karuzas <simonas.karuzas@mesh.xyz>",
  "contributors": [
    {
      "name": "Jason Healy",
      "email": "jason.healy@mesh.xyz"
    }
