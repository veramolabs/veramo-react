{
  "name": "@veramo-community/veramo-react",
  "version": "1.0.64",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@veramo/core": "3.1.2-next.91",
    "@veramo/credential-w3c": "3.1.2-next.91",
