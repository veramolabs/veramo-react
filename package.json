{
  "name": "@veramo-community/veramo-react",
  "version": "1.6.7",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@veramo/core": "6.0.1-next.40",
    "@veramo/remote-client": "6.0.1-next.40",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "7.25.8",
    "@babel/preset-react": "7.25.7",
    "@semantic-release/commit-analyzer": "13.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.1",
    "@testing-library/jest-dom": "6.6.2",
    "@testing-library/react-hooks": "8.0.1",
    "@types/jest": "29.5.13",
    "@types/react": "19.0.1",
    "@types/react-test-renderer": "19.0.0",
    "@types/uuid": "10.0.0",
    "babel-jest": "30.0.0",
    "jest": "30.0.0",
    "jest-environment-jsdom": "30.0.0",
    "prettier": "3.3.3",
    "react-test-renderer": "19.0.0",
    "semantic-release": "24.1.3",
    "ts-jest": "29.2.5",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "5.6.3",
    "uuid": "^11.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -b --watch",
    "format": "prettier --write \"src/**/*.*[jt]s*\"",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --experimental-import-meta-resolve\" jest --config=jest.cjs --coverage=false",
    "test:watch": "NODE_OPTIONS=\"--experimental-vm-modules --experimental-import-meta-resolve\" pnpm test --watch --verbose",
    "upgrade-veramo": "pnpm add -D @veramo/core@next @veramo/remote-client@next",
    "release": "semantic-release"
  },
  "prettier": {
    "jsxBracketSameLine": false,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": true,
    "semi": false
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Simonas Karuzas <simonas.karuzas@mesh.xyz>",
  "contributors": [
    "Jason Healy",
    "Mircea Nistor mircea.nistor@mesh.xyz"
  ]
}
